# Заголовки для Brotli-сжатых файлов
*.js.br
  Content-Type: application/javascript
  Content-Encoding: br
  Vary: Accept-Encoding

*.wasm.br
  Content-Type: application/wasm
  Content-Encoding: br
  Vary: Accept-Encoding

*.data.br
  Content-Type: application/octet-stream
  Content-Encoding: br
  Vary: Accept-Encoding 